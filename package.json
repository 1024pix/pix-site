{
  "name": "pix-site",
  "version": "3.18.0",
  "private": false,
  "description": "Service en ligne d’évaluation et de certification des compétences numériques pour tous, tout au long de la vie.",
  "repository": {
    "type": "git",
    "url": "git@github.com:1024pix/pix-site.git"
  },
  "license": "AGPL-3.0",
  "author": "Team PIX",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf node_modules .nuxt",
    "build": "./scripts/build.sh && npm run signal-github",
    "build:static": "nuxt generate --fail-on-error",
    "build:ssr": "nuxt build",
    "start": "nuxt start",
    "dev:site": "SITE=pix-site nuxt",
    "build:site": "SITE=pix-site nuxt generate --fail-on-error",
    "dev:pro": "SITE=pix-pro nuxt",
    "build:pro": "SITE=pix-pro nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "SITE=pix-site jest --no-coverage",
    "release": "npm run release:minor",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\"",
    "signal-github": "./scripts/signal_deploy_to_pr.sh"
  },
  "dependencies": {
    "assets-extractor-module": "1024pix/assets-extractor-module",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/prismic": "^1.3.1",
    "chart.js": "^2.9.4",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.1",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "^6.27.2",
    "nuxt-matomo": "^1.2.4",
    "nuxt-winston-log": "^1.2.0",
    "postcss-cli": "^8.3.1",
    "vue-burger-menu": "2.0.5",
    "vue-chartjs": "^3.5.1",
    "vue-js-modal": "1.3.35",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/moment": "1.6.1",
    "@nuxtjs/style-resources": "^1.2.0",
    "@sindresorhus/slugify": "^1.1.0",
    "@vue/test-utils": "^1.2.1",
    "autoprefixer": "^10.2.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^10.0.0",
    "get-urls": "^10.0.0",
    "install": "^0.13.0",
    "jest": "^27.0.6",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.2",
    "sass-loader": "^10.1.1",
    "vue-jest": "^3.0.7"
  },
  "engines": {
    "node": "14.16.0"
  }
}
