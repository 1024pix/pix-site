{
  "name": "pix-site",
  "version": "1.4.0",
  "private": false,
  "description": "La FAQ de Pix, plateforme en ligne d’évaluation et de certification des compétences numériques pour tous les français.",
  "repository": {
    "type": "git",
    "url": "git@github.com:1024pix/pix-site.git"
  },
  "license": "AGPL-3.0",
  "author": "Team PIX",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "npx ember build --environment production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint ./*.js app blueprints config lib server tests",
    "start": "npx ember serve --environment production --liveReload false --port $PORT",
    "test": "npx ember test",
    "dev": "npx ember serve",
    "upgrade": "ember update",
    "release": "npm run release:minor",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\"",
    "preversion": "git checkout dev && git pull --rebase && npm test",
    "postversion": "git push && git checkout master && git pull && git fetch -t && git merge dev --no-edit && git push origin master && git push --tags && git checkout dev"
  },
  "devDependencies": {
    "@ember/jquery": "^0.6.1",
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "cookieconsent": "^3.1.1",
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.4.1",
    "ember-burger-menu": "^3.3.3",
    "ember-cli": "^3.8.3",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.8.0",
    "ember-cli-chartjs": "^3.0.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dotenv": "^2.2.3",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-head": "^0.4.1",
    "ember-cli-htmlbars": "^3.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-matomo-tag-manager": "^1.1.2",
    "ember-cli-moment-shim": "^3.7.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.8.1",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "^3.8.3",
    "eslint": "^5.16.0",
    "eslint-plugin-ember": "^6.7.1",
    "hamburgers": "^1.1.3",
    "loader.js": "^4.7.0",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.0.3",
    "qunit-dom": "^0.8.5",
    "reset-css": "^4.0.1",
    "sass": "^1.22.3"
  },
  "engines": {
    "node": "10.15.3"
  }
}
