{
  "name": "pix-site",
  "version": "4.1.3",
  "private": false,
  "description": "Service en ligne d’évaluation et de certification des compétences numériques pour tous, tout au long de la vie.",
  "repository": {
    "type": "git",
    "url": "git@github.com:1024pix/pix-site.git"
  },
  "license": "AGPL-3.0",
  "author": "Team PIX",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf .nuxt dist",
    "build": "SITE_DOMAIN=pix.fr nuxt generate --fail-on-error && SITE_DOMAIN=pix.org nuxt generate --fail-on-error && npm run signal-jira",
    "dev:site": "PORT=7000 SITE=pix-site SITE_DOMAIN=pix.fr nuxt",
    "build:site": "SITE=pix-site SITE_DOMAIN=pix.fr nuxt generate --fail-on-error",
    "start:site": "PORT=7000 SITE=pix-site SITE_DOMAIN=pix.fr nuxt start",
    "dev:site:org": "PORT=8000 SITE=pix-site SITE_DOMAIN=pix.org nuxt",
    "build:site:org": "SITE=pix-site SITE_DOMAIN=pix.org nuxt generate --fail-on-error",
    "start:site:org": "PORT=8000 SITE=pix-site SITE_DOMAIN=pix.org nuxt start",
    "dev:pro": "PORT=7000 SITE=pix-pro SITE_DOMAIN=pix.fr nuxt",
    "build:pro": "SITE=pix-pro SITE_DOMAIN=pix.fr nuxt generate --fail-on-error",
    "start:pro": "PORT=7000 SITE=pix-pro SITE_DOMAIN=pix.fr nuxt start",
    "dev:pro:org": "PORT=8000 SITE=pix-pro SITE_DOMAIN=pix.org nuxt",
    "build:pro:org": "SITE=pix-pro SITE_DOMAIN=pix.org nuxt generate --fail-on-error",
    "start:pro:org": "PORT=8000 SITE=pix-pro SITE_DOMAIN=pix.org nuxt start",
    "start:nginx:e2e": "PORT=80 erb servers.conf.erb > nginx.conf && docker-compose up",
    "build:site:e2e": "DOMAIN_FR=localhost.fr DOMAIN_ORG=localhost.org npm run build:site && DOMAIN_FR=localhost.fr DOMAIN_ORG=localhost.org npm run build:site:org",
    "build:pro:e2e": "DOMAIN_FR=localhost.fr DOMAIN_ORG=localhost.org npm run build:pro && DOMAIN_FR=localhost.fr DOMAIN_ORG=localhost.org npm run build:pro:org",
    "dev:site:e2e": "npm run build:site:e2e && npm run start:nginx:e2e",
    "dev:pro:e2e": "npm run build:pro:e2e && npm run start:nginx:e2e",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "SITE=pix-site jest --no-coverage",
    "signal-jira": "node ./scripts/jira/comment-with-review-app-url.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/i18n": "^7.1.0",
    "@nuxtjs/prismic": "^1.4.2",
    "chart.js": "^2.9.4",
    "nuxt": "^2.15.1",
    "nuxt-winston-log": "^1.2.0",
    "postcss-cli": "^9.0.1",
    "vue-burger-menu": "2.0.5",
    "vue-chartjs": "^3.5.1",
    "vue-js-modal": "1.3.35",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@nuxt/image": "^0.6.0",
    "@nuxtjs/dayjs": "^1.4.1",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@vue/test-utils": "^1.2.2",
    "autoprefixer": "^10.4.0",
    "axios": "^0.27.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fibers": "^5.0.0",
    "install": "^0.13.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^10.1.1",
    "vue-jest": "^3.0.7"
  },
  "engines": {
    "node": "16",
    "npm": ">=8.13.2 <9"
  }
}
